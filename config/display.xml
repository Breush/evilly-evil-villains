<?xml version="1.0"?>
<config type="display">
	<group type="window">
		<param name="fullscreen" enabled="false" />
		<param name="resolution" width="1360" height="768" />
		<param name="antialiasing" level="0" />
	</group>
	<group type="nui">
		<param name="size" value="2" />
		<param name="fontFactor" value="3." />
	</group>
	<group type="global">
		<param name="language" code="en_EN" />
		<param name="scrollingFactor" value="20.f" />
		<param name="zoomSpeed" value="0.05f" />
	</group>
</config>
